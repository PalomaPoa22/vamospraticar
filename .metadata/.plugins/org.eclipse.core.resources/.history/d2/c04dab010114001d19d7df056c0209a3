package br.atividade.model;

public class Media {

	
	
	private double nota1,nota2;

	public Media(double nota1, double nota2) {
		super();
		this.nota1 = nota1;
		this.nota2 = nota2;
	}

	public double getNota1() {
		return nota1;
	}

	public double getNota2() {
		return nota2;
	}

	public void media(double nota1,double nota2) {
		double media=(nota1+nota2)/2;
		
		if(media>7) {
			System.out.println("Aprovado - nota : "+media);
		}else {
			System.out.println("Reprovado - nota : "+media);
		}
	}
	@Override
	public String toString() {
		return "Media [nota1=" + nota1 + ", nota2=" + nota2 + "]";
	}
	
	
}
