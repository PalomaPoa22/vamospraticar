package br.atividade.model;

public class Numero {

	private Integer numero;

	public Numero() {
		super();
	}

	public Integer getNumero() {
		return numero;
	}

	public void limitador(int numero) {
		if (numero > 0 && numero < 21) {
			System.out.println("Numero esta entre no limite 1-20 " + numero);
		} else {
			System.out.println("Numero fora do limite 1-20 " + numero);
		}
	}

}
